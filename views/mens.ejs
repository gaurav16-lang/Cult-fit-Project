<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="https://static.cure.fit/assets/images/curefit-v-man.svg">
    <link rel="stylesheet" href="/mens.css" />
    <link
        href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,300;1,400;1,600;1,800&display=swap"
        rel="stylesheet" />

    <title>Men</title>
</head>

<body>
    <!-- nav bar -->
    <div id="navBar">
        <a href="/">
        <div class="nav-CultFit-Logo" onclick="homePage()">
            <img src="https://static.cure.fit/assets/images/curefit-v-man.svg" class="nav-CultFit-Logo" onclick="homePage()" />
        </div>
</a>
        <!-- Nav Middle items -->
        <a href="/store"><div class="nav-middle">
            <h1 class="cultText" onclick="backToStorePage()">cultsport</h1>
        </div>
</a>
        <!-- right -->
        <div class="nav-right">
            <div class="nav-login-Logo">
                <img src="https://static.cure.fit/assets/images/user-image.svg" alt="" />
            </div>

            <div class="nav-cart-Logo">
                <img src="https://static.cure.fit/assets/images/cart-image.svg" alt="" onclick="toCart()" />
            </div>
        </div>
    </div>

    <!-- Main Sports Page -->
    <div id="main-container">
        <p style="font-weight: 500;">MEN</p>
        <div id="data">
        <% product.forEach(el=> { %>
            <a href="/store/mens/cultsport/<%= el._id %> ">
                    <div class="cultGearMenDiv">
                        <img class="productImage" src="<%= el.mainImg %>" />
                        <p class="productBrand">
                            <%= el.brand %>
                        </p>
                        <p class="productName">
                            <%= el.name %>
                        </p>
                        <div class="priceDiv">
                            <p class="productPrice">₹<%= el.price %>
                            </p>
                            <p class="productDiscountPrice">₹<%= el.discountPrice %>
                            </p>
                            <p class="productDiscount">
                                <%= el.discount %>% OFF
                            </p>
                        </div>
                    </div>
                    </a>
            <% }) %>
        </div>
    </div>

    <!-- footer -->
    <%- include("../components/footer") %>
        <!-- footer ends -->

</body>

</html>
